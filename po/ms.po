msgid "Today"
msgstr "Hari Ini"

#: ../catfish/CatfishWindow.py:1906
msgid "Yesterday"
msgstr "Semalam"

#: ../catfish/CatfishWindow.py:2014
msgid "No files found."
msgstr "Tiada fail ditemui."

#: ../catfish/CatfishWindow.py:2016
msgid ""
"Try making your search less specific\n"
"or try another directory."
msgstr ""
"Cuba jadikan penggelintaran anda kurang\n"
"spesifik atau cuba pada direktori lain."

#: ../catfish/CatfishWindow.py:2023
msgid "1 file found."
msgstr "1 fail ditemui."

#: ../catfish/CatfishWindow.py:2025
#, python-format
msgid "%i files found."
msgstr "%i fail ditemui."

