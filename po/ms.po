msgstr "\"%s\" tidak dapat dibuka."

#: ../catfish/CatfishWindow.py:1483
#, python-format
msgid "\"%s\" could not be saved."
msgstr "\"%s\" tidak dapat disimpan."

#: ../catfish/CatfishWindow.py:1500
#, python-format
msgid "\"%s\" could not be deleted."
msgstr "\"%s\" tidak dapat dipadamkan."

